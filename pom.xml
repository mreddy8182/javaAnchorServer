<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.0.6.RELEASE</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <groupId>me.kroeker.alex.anchor</groupId>
    <artifactId>j-server</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <packaging>pom</packaging>

    <name>anchor-j-server</name>
    <description>Demo project for Spring Boot</description>

    <modules>
        <module>j-server-application</module>
        <module>j-server-model</module>
        <module>j-server-service</module>
        <module>j-server-controller</module>
        <module>j-server-dao</module>
        <module>j-server-business</module>
        <module>j-server-dao-spark</module>
        <module>j-server-dao-h2o</module>
    </modules>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <java.version>1.8</java.version>
    </properties>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-logging</artifactId>
        </dependency>
    </dependencies>

    <dependencyManagement>
        <dependencies>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-model</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-service</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-controller</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-dao</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-business</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-dao-spark</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
            <dependency>
                <groupId>me.kroeker.alex.anchor</groupId>
                <artifactId>j-server-dao-h2o</artifactId>
                <version>0.0.1-SNAPSHOT</version>
            </dependency>
        </dependencies>
